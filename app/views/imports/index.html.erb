<h1>Listing imports</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Imported by</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @imports.each do |import| %>
      <tr>
        <td><%= import.title %></td>
        <td><%= import.user.try(:email) %></td>
        <td><%= link_to 'Show', import %>
        | <%= link_to 'Destroy', import, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Import', new_import_path %>
